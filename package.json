    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@iconify-json/ep": "^1.1.15",
    "@iconify/vue": "^4.1.2",
    "@types/node": "^20.11.30",
    "@vitejs/plugin-vue": "^5.2.0",
    "axios": "^1.7.9",
    "sass": "^1.72.0",
    "typescript": "^5.4.3",
    "unocss": "^0.58.6",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.2.5",
    "vite-ssg": "^0.23.6",
